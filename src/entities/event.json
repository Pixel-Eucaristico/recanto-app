{
    "name": "Event",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "description": "Nome do evento ou compromisso"
      },
      "description": {
        "type": "string",
        "description": "Detalhes sobre o evento"
      },
      "start_time": {
        "type": "string",
        "format": "date-time"
      },
      "end_time": {
        "type": "string",
        "format": "date-time"
      },
      "type": {
        "type": "string",
        "enum": [
          "oracao",
          "formacao",
          "reuniao",
          "evento_aberto"
        ],
        "description": "Tipo de compromisso"
      }
    },
    "required": [
      "title",
      "start_time"
    ]
  }